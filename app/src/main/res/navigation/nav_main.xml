<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.android.t4tek.app.braingame.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_listGameFragment"
            app:destination="@id/listGameFragment" />
    </fragment>

    <fragment
        android:id="@+id/listGameFragment"
        android:name="com.android.t4tek.app.braingame.list_game.ListGameFragment"
        android:label="fragment_list_game"
        tools:layout="@layout/fragment_list_game" >
        <action
            android:id="@+id/action_listGameFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_listGameFragment_to_mathGameFragment"
            app:destination="@id/mathGameFragment" />
    </fragment>

    <fragment
        android:id="@+id/mathGameFragment"
        android:name="com.android.t4tek.app.braingame.mathgame.MathGameFragment"
        android:label="fragment_math_game"
        tools:layout="@layout/fragment_math_game" >
        <action
            android:id="@+id/action_mathGameFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>

    <fragment
        android:id="@+id/resultFragment"
        android:name="com.android.t4tek.app.braingame.result.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" >
        <action
            android:id="@+id/action_resultFragment_to_listRSFragment"
            app:destination="@id/listRSFragment" />
    </fragment>
    <fragment
        android:id="@+id/listRSFragment"
        android:name="com.android.t4tek.app.braingame.listresult.ListRSFragment"
        android:label="fragment_list_r_s"
        tools:layout="@layout/fragment_list_r_s" />

</navigation>