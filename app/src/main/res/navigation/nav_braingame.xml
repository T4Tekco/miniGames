<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_braingame"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.android.t4tek.app.braingame.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_listGameFragment"
            app:destination="@id/listGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/listGameFragment"
        android:name="com.android.t4tek.app.braingame.list_game.ListGameFragment"
        android:label="fragment_list_game"
        tools:layout="@layout/fragment_list_game" >
        <action
            android:id="@+id/action_listGameFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>